{
  "name": "iboy",
  "version": "0.1.3",
  "description": "A simple CLI scaffolding for front-end projects",
  "main": "index.js",
  "bin": {
    "iboy": "bin/iboy",
    "iboy-init": "bin/iboy-init",
    "iboy-token": "bin/iboy-token"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -w -r 0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sosout/iboy.git"
  },
  "keywords": [
    "iboy",
    "cli",
    "spa",
    "scaffold",
    "front end scaffold"
  ],
  "author": "weich",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sosout/iboy/issues"
  },
  "homepage": "https://github.com/sosout/iboy#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "chalk": "^2.4.1",
    "commander": "^2.16.0",
    "consolidate": "^0.15.1",
    "download-git-repo": "^1.0.2",
    "handlebars": "^4.0.11",
    "inquirer": "^6.0.0",
    "metalsmith": "^2.3.0",
    "ora": "^3.0.0",
    "read-metadata": "^1.0.0",
    "request": "^2.87.0",
    "rimraf": "^2.6.2",
    "semver": "^5.5.0",
    "shelljs": "^0.8.2",
    "uuid": "^3.3.2",
    "validate-npm-package-name": "^3.0.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.0.0-rc.13"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
